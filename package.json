{
  "name": "nuxt3-recaptcha",
  "version": "0.0.1",
  "description": "Nuxt 3 integration for Google Recaptcha",
  "repository": "arashsheyda/nuxt-recaptcha",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:host": "nuxi dev playground --host auctria.local --https",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run build && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint ."
  },
  "dependencies": {
    "@nuxt/kit": "^3.10.3",
    "defu": "^6.1.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.0.8",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.10.3",
    "@nuxt/ui": "^2.14.2",
    "@types/node": "^20.11.24",
    "changelogen": "^0.5.5",
    "eslint": "^8.57.0",
    "nuxt": "^3.10.3",
    "typescript": "^5.3.3",
    "vue": "^3.4.21"
  }
}